{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "no-console": [
            false
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "linebreak-style": [
            true,
            "LF"
        ]
    }
}
